@{
    ViewData["Title"] = "Availibility";
    Layout = "~/Views/Shared/_VendorLayout.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vendor Dashboard</title>
    <!-- Bootstrap CSS -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <style>
        .container {
            margin-top: 50px;
        }
    </style>
</head>
<body>

    <div class="container">
        <h2 class="mb-4">Vendor Dashboard</h2>
        <div class="row">
            <div class="col-md-6">
                <!-- Display bookings here -->
                <h4>Bookings</h4>
                <ul class="list-group">
                    <!-- Dummy data -->
                    <li class="list-group-item">Date: 2024-06-30 | Time: 09:00 - 10:00 | Customer: John Doe</li>
                    <li class="list-group-item">Date: 2024-07-01 | Time: 10:00 - 11:00 | Customer: Jane Smith</li>
                </ul>
            </div>
            <div class="col-md-6">
                <!-- Display additional information -->
                <h4>Additional Information</h4>
                <ul class="list-group">
                    <!-- Dummy data -->
                    <li class="list-group-item">Total Bookings: 10</li>
                    <li class="list-group-item">Revenue: $1000</li>
                    <li class="list-group-item">Pending Requests: 3</li>
                </ul>
            </div>
        </div>
    </div>

    <!-- Bootstrap JS and dependencies -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>

    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

</body>
</html>
