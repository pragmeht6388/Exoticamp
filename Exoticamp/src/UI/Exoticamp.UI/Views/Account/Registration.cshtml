@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            // Add custom validation method for Terms and Conditions checkbox
            $.validator.addMethod("checkTerms", function (value, element) {
                return $(element).is(":checked");
            }, "You must accept the terms and conditions.");

            // Custom validation method to detect numeric input in Name field
            $.validator.addMethod("noNumbersInName", function (value, element) {
                return !/\d/.test(value); // Check if value contains any digits
            }, "Name cannot contain numbers.");

            // Custom validation method to trigger error for "plz" in Name field
            $.validator.addMethod("noPlz", function (value, element) {
                return value.toLowerCase().indexOf("plz") === -1; // Case insensitive check for "plz"
            }, "Please do not use 'plz' in your name.");

            // Apply custom validation rules
            $("#registrationForm").validate({
                rules: {
                    Name: {
                        required: true,
                        maxlength: 50,
                        noNumbersInName: true,
                        noPlz: true
                    },
                    PhoneNumber: {
                        required: true,
                        digits: true,
                        minlength: 10,
                        maxlength: 10
                    },
                    Email: {
                        required: true,
                        email: true
                    },
                    TermsandCondition: {
                        checkTerms: true // Ensure TermsandCondition is required and checked
                    },
                    Password: {
                        required: true,
                        minlength: 6
                    }
                },
                messages: {
                    Name: {
                        required: "Name is required.",
                        maxlength: "Name cannot be longer than 50 characters.",
                        noNumbersInName: "Name cannot contain numbers.",
                        noPlz: "Please do not use 'plz' in your name."
                    },
                    PhoneNumber: {
                        required: "Phone number is required.",
                        digits: "Phone number must contain digits only.",
                        minlength: "Phone number must be exactly 10 digits.",
                        maxlength: "Phone number must be exactly 10 digits."
                    },
                    Email: {
                        required: "Email is required.",
                        email: "Invalid email address."
                    },
                    TermsandCondition: {
                        checkTerms: "You must accept the terms and conditions."
                    },
                    Password: {
                        required: "Password is required.",
                        minlength: "Password must be at least 6 characters long."
                    }
                },
                errorPlacement: function (error, element) {
                    if (element.attr("type") === "checkbox") {
                        error.appendTo(element.parent());
                    } else {
                        error.insertAfter(element);
                    }
                }
            });

            // Keydown event listener for Name field
            $("#Name").on("keydown", function (e) {
                var key = e.key;

                // Check if the key pressed is a number
                if (!isNaN(parseInt(key))) {
                    e.preventDefault(); // Prevent entering numbers
                    $("#Name-error").html("Name cannot contain numbers.");
                    $(this).addClass("input-validation-error");
                } else {
                    $("#Name-error").html("");
                    $(this).removeClass("input-validation-error");
                }
            });
        });
    </script>
}

